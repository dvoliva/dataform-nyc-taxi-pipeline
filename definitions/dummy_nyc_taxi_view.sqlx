config {
    type: "view",
    schema: "nyc_taxi_dataform",
    name: "dummy_nyc_taxi_view"
}

SELECT
  vendor_id,
  pickup_datetime,
  dropoff_datetime,
  passenger_count,
  trip_distance,
  payment_type,
  total_amount
FROM
  `bigquery-public-data.new_york_taxi_trips.tlc_yellow_trips_2017`
LIMIT
  100
